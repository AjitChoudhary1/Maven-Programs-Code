<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Grouping TestCase System,Smoke Together">
	<test thread-count="5" name="System,Smoke Test">
		<groups>
			<run>
				<include name="smoke"></include>
				<exclude name="system"></exclude>
			</run>
		</groups>
		<classes>
			<class name="maven_code.Grouping_TestCases" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
